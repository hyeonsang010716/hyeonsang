class Solution {
    public String solution(String str1, String str2) {
        //char answer = new char('');
        int n = 0;
        int cnt1 = str1.length();
        for (int i = 0; i < cnt1; i++){
            if(str1.charAt(i) != str2.charAt(i)){
                //answer = str1.charAt(i);
                n = i;
            }
        }
        
        return Character.toString(str1.charAt(n));
    }
}
